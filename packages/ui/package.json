{
  "name": "@sdbank/ui",
  "version": "0.1.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./next": "./src/next/index.ts",
    "./web": "./src/next/react-native-shim.tsx",
    "./global.css": "./global.css"
  },
  "files": [
    "dist",
    "src",
    "loaders",
    "global.css"
  ],
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./next": {
        "import": "./dist/next/index.mjs",
        "require": "./dist/next/index.js",
        "types": "./dist/next/index.d.ts"
      },
      "./web": "./src/next/react-native-shim.tsx",
      "./global.css": "./global.css"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.74.0",
    "react-native-web": ">=0.19.0"
  },
  "peerDependenciesMeta": {
    "react-native-web": {
      "optional": true
    }
  },
  "dependencies": {
    "uniwind": "^1.3.2",
    "tailwindcss": "^4.1.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.0.0"
  },
  "devDependencies": {
    "@sdbank/eslint-config": "*",
    "@sdbank/typescript-config": "*",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/node": "^25.3.0",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.39.1",
    "jsdom": "^26.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-web": "^0.21.0",
    "react-test-renderer": "19.1.0",
    "tsup": "^8.5.1",
    "typescript": "5.9.2",
    "vitest": "^3.2.1"
  }
}
